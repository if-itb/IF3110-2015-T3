        <servlet-name>VoteQuestionServlet</servlet-name>
        <servlet-class>Question.VoteQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VoteAnswerServlet</servlet-name>
        <servlet-class>Answer.VoteAnswerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeaderServlet</servlet-name>
        <servlet-class>Header.HeaderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>User.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreatePageServlet</servlet-name>
        <servlet-class>Question.CreatePageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionListServlet</servlet-name>
        <url-pattern>/QuestionListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateQuestionServlet</servlet-name>
        <url-pattern>/CreateQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionByQIDServlet</servlet-name>
        <url-pattern>/QuestionByQIDServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAnswerServlet</servlet-name>
        <url-pattern>/CreateAnswerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditQuestionServlet</servlet-name>
        <url-pattern>/EditQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditServlet</servlet-name>
        <url-pattern>/EditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteQuestionServlet</servlet-name>
        <url-pattern>/DeleteQuestionServlet</url-pattern>
