    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreatePageServlet</servlet-name>
        <url-pattern>/CreatePageServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
